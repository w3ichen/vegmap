<model name="bush_spring">
  <static>false</static>
  <link name="bush_link">
    <pose>2 2 0.5 0 0 0</pose>

    <collision name="collision">
      <geometry>
        <cylinder>
          <radius>0.5</radius>
          <length>1.0</length>
        </cylinder>
      </geometry>
    </collision>

    <visual name="visual">
      <geometry>
        <cylinder>
          <radius>0.5</radius>
          <length>1.0</length>
        </cylinder>
      </geometry>
      <material>
        <ambient>0.2 0.8 0.2 1</ambient>
      </material>
    </visual>

    <!-- ðŸ‘‡ Contact Sensor -->
    <sensor name="contact_sensor" type="contact">
      <always_on>1</always_on>
      <update_rate>30</update_rate>
      <contact>
        <collision>collision</collision>
      </contact>
      <plugin name="contact_plugin" filename="libgazebo_ros_bumper.so">
        <ros>
          <namespace>/bush_spring</namespace>
          <remapping>~/bumper:=contact</remapping>
        </ros>
      </plugin>
    </sensor>

  </link>

  <joint name="spring_joint" type="prismatic">
    <parent>world</parent>
    <child>bush_link</child>
    <axis>
      <xyz>0 0 1</xyz>
      <limit>
        <lower>-0.1</lower>
        <upper>0.1</upper>
      </limit>
      <dynamics>
        <spring_stiffness>100.0</spring_stiffness>
        <damping>5.0</damping>
      </dynamics>
    </axis>
  </joint>
</model>
